    <SpecularPower>24</SpecularPower>
    <SpecularShininess>0.364</SpecularShininess>
    <ColorMetalXZnY>Textures\Voxels\GrassAlkaline_cm.dds</ColorMetalXZnY>
    <NormalGlossXZnY>Textures\Voxels\GrassAlkaline_ng.dds</NormalGlossXZnY>
    <ExtXZnY>Textures\Voxels\GrassAlkaline_add.dds</ExtXZnY>
    
    <ColorMetalXZnYFar1>Textures\Voxels\ExhaustedSoil_Far1_cm.dds</ColorMetalXZnYFar1>
    <NormalGlossXZnYFar1>Textures\Voxels\ExhaustedSoil_Far1_ng.dds</NormalGlossXZnYFar1>
    <ExtXZnYFar1>Textures\Voxels\ExhaustedSoil_Far1_add.dds</ExtXZnYFar1>

    <ColorMetalXZnYFar2>Textures\Voxels\ExhaustedSoil_Far1_cm.dds</ColorMetalXZnYFar2>
    <NormalGlossXZnYFar2>Textures\Voxels\ExhaustedSoil_Far1_ng.dds</NormalGlossXZnYFar2>
    <ExtXZnYFar2>Textures\Voxels\ExhaustedSoil_Far1_add.dds</ExtXZnYFar2>

    <FoliageColorTextureArray>
      <Color>Textures\Models\Environment\Grass\Grass_PlantedSoil_2_cm.dds</Color>
    </FoliageColorTextureArray>

    <FoliageNormalTextureArray>
      <Normal>Textures\Models\Environment\Grass\Grass_PlantedSoil_2_ng.dds</Normal>
    </FoliageNormalTextureArray>

    <FoliageDensity>6</FoliageDensity>
    <FoliageScale>
      <X>0.09</X>
      <Y>0.09</Y>
    </FoliageScale>
    <FoliageRandomRescaleMult>0.01</FoliageRandomRescaleMult>

    <!-- Far Distance -->
    <Far1Distance>70</Far1Distance>
    <Far2Distance>500</Far2Distance>
    <Far1Scale>65</Far1Scale>
    <Far2Scale>300</Far2Scale>
    
    <Foliage Density="5" Type="Grass">
      <Size x="0.6" y="0.8" />
      <SizeVariation>.6</SizeVariation>

      <Variation Color="Textures\Models\Environment\Grass\SteppeYellowGrass_Grass_1_cm.dds" Normal="Textures\Models\Environment\Grass\SteppeYellowGrass_Grass_1_ng.dds" Frequency="1" />
